
<h1 align="center">Log In</h1>
<div align="center">
<%= form_for(:session, url: '/') do |f| %>
    <%= f.text_field :username, :placeholder => "Username",class:"inputbox" %>
    <%= f.password_field :password, :placeholder => "Password",class:"inputbox" %>
    <p><%= f.submit "Login", class: "buttons" %></p>
<% end %>
<%= link_to "Signup", '/signup', class:"buttons" %>
</div>
